package chp10_5;

public class MathTest {

	public static void main(String[] args) {
		
		double r1 = Math.random();
		int r2 = (int) (Math.random()*5);
		int x = Math.abs(-240);
		double d = Math.abs(240.45);
		
		System.out.println(r1+" "+r2+" "+x+" "+d);
		System.out.println(Math.round(-24.8));

	}

}
